<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
      "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="StoryList" class="com.springtest.app.eclipsemaventest.StoryList">
      <property name="stories">
         <list>
           <ref bean="HelloWorld"/>
           <ref bean="OnceUpon"/>
         </list>
       </property>
   </bean>
   <!-- Remember to import in the 'beans' code when you need those beans -->
   <import resource="story_configuration.xml"/>
   
</beans>